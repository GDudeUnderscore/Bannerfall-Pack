{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/stone_shovel"
        },
        "cases": [
            {"when": "flint_spear","model": {"type": "model","model": "item/custom/weapons/melee/spears/flint_spear"}},
            {"when": "venomspire","model": {"type": "model","model": "item/custom/weapons/melee/staffs/venomspire"}},
            {"when": "stonespike","model": {"type": "model","model": "item/custom/weapons/melee/spears/stonespike"}}
        ]
    }
}