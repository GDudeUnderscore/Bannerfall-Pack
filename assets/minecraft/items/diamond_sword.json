{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/diamond_sword"
        },
        "cases": [
            {"when": "ancient_scimitar","model": {"type": "model","model": "item/custom/weapons/melee/swords/ancient_scimitar"}},
            {"when": "luna","model": {"type": "model","model": "item/custom/weapons/melee/swords/luna"}},
            {"when": "frozen_greatsword","model": {"type": "model","model": "item/custom/weapons/melee/swords/frozen_greatsword"}}
        ]
    }
}