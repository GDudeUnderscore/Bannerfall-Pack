{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/golden_sword"
        },
        "cases": [
            {"when": "arcanote","model": {"type": "model","model": "item/custom/weapons/melee/swords/arcanote"}},
            {"when": "hellfire","model": {"type": "model","model": "item/custom/weapons/melee/swords/hellfire"}},
            {"when": "thundral","model": {"type": "model","model": "item/custom/weapons/melee/swords/thundral"}},
            {"when": "resonark","model": {"type": "model","model": "item/custom/weapons/melee/swords/resonark"}},
            {"when": "sol","model": {"type": "model","model": "item/custom/weapons/melee/swords/sol"}}
        ]
    }
}