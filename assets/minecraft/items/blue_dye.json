{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/blue_dye"
        },
        "cases": [
            {"when": "common_capsule_top","model": {"type": "model","model": "item/custom/capsule/common/capsule_top"}},
            {"when": "common_capsule_bottom","model": {"type": "model","model": "item/custom/capsule/common/capsule_bottom"}},

            {"when": "rare_capsule_top","model": {"type": "model","model": "item/custom/capsule/rare/capsule_top"}},
            {"when": "rare_capsule_bottom","model": {"type": "model","model": "item/custom/capsule/rare/capsule_bottom"}},

            {"when": "legendary_capsule_top","model": {"type": "model","model": "item/custom/capsule/legendary/capsule_top"}},
            {"when": "legendary_capsule_bottom","model": {"type": "model","model": "item/custom/capsule/legendary/capsule_bottom"}}
        ]
    }
}