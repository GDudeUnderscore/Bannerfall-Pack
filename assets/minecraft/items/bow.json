{
    "model": {
        "type": "minecraft:condition",
        "property": "minecraft:using_item",

        "on_false": {
            "type": "select", "property": "custom_model_data",
            "fallback": { "type": "model", "model": "item/bow" },
            "cases": [
                { "when": "flimsy_bow", "model": { "type": "model", "model": "item/custom/flimsy_bow/flimsy_bow" }},
                { "when": "reinforced_bow", "model": { "type": "model", "model": "item/custom/reinforced_bow/reinforced_bow" }},
                { "when": "keen_eye", "model": { "type": "model", "model": "item/custom/keen_eye/keen_eye" }},
                { "when": "chronoweaver", "model": { "type": "model", "model": "item/custom/chronoweaver/chronoweaver" }},
                { "when": "fracture_bow", "model": { "type": "model", "model": "item/custom/fracture_bow/fracture_bow" }},
                { "when": "hexflame", "model": { "type": "model", "model": "item/custom/hexflame/hexflame" }},
                { "when": "tempest", "model": { "type": "model", "model": "item/custom/tempest/tempest" }},
                { "when": "leviathan", "model": { "type": "model", "model": "item/custom/leviathan/leviathan" }},
                { "when": "double_shooter", "model": { "type": "model", "model": "item/custom/double_shooter/double_shooter" }},
                { "when": "floodbringer", "model": { "type": "model", "model": "item/custom/floodbringer/floodbringer" }},
                { "when": "twilight", "model": { "type": "model", "model": "item/custom/twilight/twilight" }}
            ]
        },

        "on_true": {
            "type": "minecraft:range_dispatch",

            "entries": [
                { "model": { "type": "select", "property": "custom_model_data", "fallback": { "type": "model", "model": "item/bow_pulling_1"}, "cases": [
                    { "when": "flimsy_bow", "model": { "type": "model", "model": "item/custom/flimsy_bow/flimsy_bow_pulling_1" }},
                    { "when": "reinforced_bow", "model": { "type": "model", "model": "item/custom/reinforced_bow/reinforced_bow_pulling_1" }},
                    { "when": "keen_eye", "model": { "type": "model", "model": "item/custom/keen_eye/keen_eye_pulling_1" }},
                    { "when": "chronoweaver", "model": { "type": "model", "model": "item/custom/chronoweaver/chronoweaver_pulling_1" }},
                    { "when": "fracture_bow", "model": { "type": "model", "model": "item/custom/fracture_bow/fracture_bow_pulling_1" }},
                    { "when": "hexflame", "model": { "type": "model", "model": "item/custom/hexflame/hexflame_pulling_1" }},
                    { "when": "tempest", "model": { "type": "model", "model": "item/custom/tempest/tempest_pulling_1" }},
                    { "when": "leviathan", "model": { "type": "model", "model": "item/custom/leviathan/leviathan_pulling_1" }},
                    { "when": "double_shooter", "model": { "type": "model", "model": "item/custom/double_shooter/double_shooter_pulling_1" }},
                    { "when": "floodbringer", "model": { "type": "model", "model": "item/custom/floodbringer/floodbringer_pulling_1" }},
                    { "when": "twilight", "model": { "type": "model", "model": "item/custom/twilight/twilight_pulling_1" }}

                ]}, "threshold": 0.65 },

                { "model": { "type": "select", "property": "custom_model_data", "fallback": { "type": "model", "model": "item/bow_pulling_2"}, "cases": [
                    { "when": "flimsy_bow", "model": { "type": "model", "model": "item/custom/flimsy_bow/flimsy_bow_pulling_2" }},
                    { "when": "reinforced_bow", "model": { "type": "model", "model": "item/custom/reinforced_bow/reinforced_bow_pulling_2" }},
                    { "when": "keen_eye", "model": { "type": "model", "model": "item/custom/keen_eye/keen_eye_pulling_2" }},
                    { "when": "chronoweaver", "model": { "type": "model", "model": "item/custom/chronoweaver/chronoweaver_pulling_2" }},
                    { "when": "fracture_bow", "model": { "type": "model", "model": "item/custom/fracture_bow/fracture_bow_pulling_2" }},
                    { "when": "hexflame", "model": { "type": "model", "model": "item/custom/hexflame/hexflame_pulling_2" }},
                    { "when": "tempest", "model": { "type": "model", "model": "item/custom/tempest/tempest_pulling_2" }},
                    { "when": "leviathan", "model": { "type": "model", "model": "item/custom/leviathan/leviathan_pulling_2" }},
                    { "when": "double_shooter", "model": { "type": "model", "model": "item/custom/double_shooter/double_shooter_pulling_2" }},
                    { "when": "floodbringer", "model": { "type": "model", "model": "item/custom/floodbringer/floodbringer_pulling_2" }},
                    { "when": "twilight", "model": { "type": "model", "model": "item/custom/twilight/twilight_pulling_2" }}
                ]}, "threshold": 0.90 }
            ],
            
            "fallback": { "type": "select", "property": "custom_model_data", "fallback": { "type": "model", "model": "item/bow_pulling_0"},
            "cases": [
                { "when": "flimsy_bow", "model": { "type": "model", "model": "item/custom/flimsy_bow/flimsy_bow_pulling_0" }},
                { "when": "reinforced_bow", "model": { "type": "model", "model": "item/custom/reinforced_bow/reinforced_bow_pulling_0" }},
                { "when": "keen_eye", "model": { "type": "model", "model": "item/custom/keen_eye/keen_eye_pulling_0" }},
                { "when": "chronoweaver", "model": { "type": "model", "model": "item/custom/chronoweaver/chronoweaver_pulling_0" }},
                { "when": "fracture_bow", "model": { "type": "model", "model": "item/custom/fracture_bow/fracture_bow_pulling_0" }},
                { "when": "hexflame", "model": { "type": "model", "model": "item/custom/hexflame/hexflame_pulling_0" }},
                { "when": "tempest", "model": { "type": "model", "model": "item/custom/tempest/tempest_pulling_0" }},
                { "when": "leviathan", "model": { "type": "model", "model": "item/custom/leviathan/leviathan_pulling_0" }},
                { "when": "double_shooter", "model": { "type": "model", "model": "item/custom/double_shooter/double_shooter_pulling_0" }},
                { "when": "floodbringer", "model": { "type": "model", "model": "item/custom/floodbringer/floodbringer_pulling_0" }},
                { "when": "twilight", "model": { "type": "model", "model": "item/custom/twilight/twilight_pulling_0" }}
            ]},

            "property": "minecraft:use_duration",
            "scale": 0.05
        }
    }
}