{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_sword"
        },
        "cases": [
            {"when": "steel_katana","model": {"type": "model","model": "item/custom/weapons/melee/swords/steel_katana"}},
            {"when": "steel_sword","model": {"type": "model","model": "item/custom/weapons/melee/swords/steel_sword"}},
            {"when": "flaming_dagger","model": {"type": "model","model": "item/custom/weapons/melee/swords/flaming_dagger"}},
            {"when": "icicle","model": {"type": "model","model": "item/custom/weapons/melee/swords/icicle"}},
            {"when": "swift_katana","model": {"type": "model","model": "item/custom/weapons/melee/swords/swift_katana"}},
            {"when": "crystal_seekers","model": {"type": "model","model": "item/custom/weapons/melee/swords/crystal_seekers"}}
        ]
    }
}