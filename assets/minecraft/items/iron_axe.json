{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_axe"
        },
        "cases": [
            {"when": "ragnarok","model": {"type": "model","model": "item/custom/weapons/melee/axes/ragnarok"}},
            {"when": "icebreaker","model": {"type": "model","model": "item/custom/weapons/melee/axes/icebreaker"}},
            {"when": "crystalline","model": {"type": "model","model": "item/custom/weapons/melee/axes/crystalline"}},
            {"when": "hookfang","model": {"type": "model","model": "item/custom/weapons/melee/axes/hookfang"}},
            {"when": "grimdark","model": {"type": "model","model": "item/custom/weapons/melee/axes/grimdark"}}
        ]
    }
}