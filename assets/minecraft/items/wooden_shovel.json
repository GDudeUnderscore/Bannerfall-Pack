{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/wooden_shovel"
        },
        "cases": [
            {"when": "divinity","model": {"type": "model","model": "item/custom/divinity"}},
            {"when": "fangspear","model": {"type": "model","model": "item/custom/fangspear"}},
            {"when": "nautilus","model": {"type": "model","model": "item/custom/nautilus"}},
            {"when": "verdant_scepter","model": {"type": "model","model": "item/custom/verdant_scepter"}},
            {"when": "frostpetal","model": {"type": "model","model": "item/custom/frostpetal"}}
        ]
    }
}