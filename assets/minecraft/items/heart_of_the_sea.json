{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/heart_of_the_sea"
        },
        "cases": [
            {"when": "common_capsule","model": {"type": "model","model": "item/custom/capsule/common/capsule"}},
            {"when": "rare_capsule","model": {"type": "model","model": "item/custom/capsule/rare/capsule"}},
            {"when": "legendary_capsule","model": {"type": "model","model": "item/custom/capsule/legendary/capsule"}}
        ]
    }
}