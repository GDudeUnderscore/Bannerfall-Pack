{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/stone_sword"
        },
        "cases": [
            {"when": "cobblestone_sword","model": {"type": "model","model": "item/custom/weapons/melee/swords/cobblestone_sword"}},
            {"when": "bloodrage","model": {"type": "model","model": "item/custom/weapons/melee/swords/bloodrage"}},
            {"when": "kelpy_cutlass","model": {"type": "model","model": "item/custom/weapons/melee/swords/kelpy_cutlass"}},
            {"when": "lament","model": {"type": "model","model": "item/custom/weapons/melee/swords/lament"}},
            {"when": "plaguebringer","model": {"type": "model","model": "item/custom/weapons/melee/swords/plaguebringer"}}
        ]
    }
}